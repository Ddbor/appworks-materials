{
  "name": "@ddbor/appworks-materials",
  "version": "0.2.0",
  "description": "这是一个AppWorks自定义物料项目",
  "author": "ddbor",
  "scripts": {
    "deploy": "npm run generate && npm run sync",
    "generate": "appworks generate",
    "sync": "appworks sync",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "prepublishOnly": "npm run generate"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/Ddbor/appworks-materials",
  "devDependencies": {
    "@ice/screenshot": "^0.1.1",
    "@iceworks/spec": "^1.0.0",
    "eslint": "^7.0.0",
    "stylelint": "^13.1.0"
  },
  "materialConfig": {
    "template": "@icedesign/ice-react-ts-material-template",
    "type": "react"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Ddbor/appworks-materials.git"
  },
  "license": "MIT"
}
